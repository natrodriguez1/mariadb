<div class="container-fluid ">

    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Título</th>
                <th>Descripción</th>
                <th>Ruta</th>
                <th>Fecha de creación</th>
                <th>Etiqueta(s)</th>
            </tr>
        </thead>
        <tbody>
            <% arrFotos.forEach((foto)=> { %>
                <tr>
                    <td>
                        <%= foto.id %>
                    </td>
                    <td>
                        <%= foto.titulo %>
                    </td>
                    <td>
                        <%= foto.descripcion %>
                    </td>
                    <td>
                        <%= foto.ruta %>
                    </td>
                    <td>
                        <%= foto.createdAt.toLocaleDateString('en-US') %>
                    </td>
                    <td>
                        <% foto.etiqueta.forEach((etiqueta)=> { %>
                            <%= etiqueta.texto %>
                        <% }); %>
                    </td>
                    
                </tr>
                <% }); %>
        </tbody>
    </table>
</div>